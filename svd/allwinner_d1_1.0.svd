                  <description>THR empty</description>
                  <value>0b0010</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>RS485</name>
                  <description>RS485 Interrupt</description>
                  <value>0b0011</value>
                </enumeratedValue>
                <enumeratedValue>
