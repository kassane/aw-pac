                  <value>0b01</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>FIFO__onehalf__full</name>
                  <description>FIFO &#189;  full</description>
                  <value>0b10</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>FIFO_minus_2_less</name>
                  <description>FIFO-2 less than full</description>
